   <% time_now = Date.today %>
<h4>Current Events</h4>

<table class="table">
  <thead class="thead-dark">
    <tr>
      <th>User</th>
      <th>Title</th>
      <th>Date</th>
      <th>Location</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <% if time_now <= event.date  %>
         
        <tr>
            <td><%= event.user.username %></td>
            <td><%= event.title %></td>
            <td><%= event.date %></td>
            <td><%= event.location %></td>
            <td><%= event.body %></td>
              <% if event.user.id ==  session[:user_id] %>
                <td><%= link_to 'Show All', event %></td>
              <% else %>
                <td></td>
              <% end %>  
        </tr>

      <% end %> 
    <% end %>
  </tbody>
</table>

